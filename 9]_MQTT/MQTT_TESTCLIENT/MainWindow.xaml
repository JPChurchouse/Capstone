<Window x:Class="MQTT_TESTCLIENT.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MQTT_TESTCLIENT"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        Closed="Window_Closed">
    <Grid>
        <Button x:Name="btn_Send" Content="Button" HorizontalAlignment="Left" Margin="71,90,0,0" VerticalAlignment="Top" Width="85" Height="43" Click="btn_Send_Click"/>
        <TextBox x:Name="txt_Send" HorizontalAlignment="Left" Margin="36,149,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="166"/>
        <TextBox x:Name="txt_Recieve" HorizontalAlignment="Left" Margin="400,0,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Center" Width="377" Height="311"/>

    </Grid>
</Window>
